{
  "compilerOptions": {
    "outDir": "./router/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es2017",
    "jsx": "react",
    "emitDecoratorMetadata": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "*": [
        "node_modules/*",
      ]
    },
    "lib": [
      "es2017"
    ]
  },
  "include": [
    "./controller/**/*"
  ]
}